<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.planner">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="Excitivity"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Planner">
        <activity android:name=".Login.PasswordInput" />
        <activity
            android:name=".MainActivity"
            android:label="Excitivity"
            android:theme="@style/Theme.Planner.NoActionBar" />
        <activity android:name=".Cooking"></activity>
        <activity android:name=".Sport"></activity>
        <activity android:name=".Study"></activity>
        <activity android:name=".Photography"></activity>
        <activity android:name=".Login.LoginActivity" />
        <activity android:name=".ProgramsActivity">
        </activity>
        <activity android:name=".SplashActivity"
            android:theme="@style/SplashScreenTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>